{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": "on",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "linebreak-style": "off",
      "no-console": "off"
    }
}